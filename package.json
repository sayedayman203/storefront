{
  "name": "storefront",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:sayedayman203/storefront.git",
  "author": "Sayed Ayman <sayedayman203@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "watch": "cross-env NODE_ENV=development tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess 'npm run start'",
    "lint": "eslint src/**/*.ts",
    "lint:f": "eslint src/**/*.ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine",
    "pretest": "db-migrate reset --env test && db-migrate up --env test",
    "test": "cross-env-shell NODE_ENV=test 'npm run build && npm run jasmine'"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "tsconfig-paths": "^3.14.1",
    "tscpaths": "^0.0.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jasmine": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.3"
  }
}
